<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].post %></td>
   <td>

        <a href="/update-user?id=<%= users[i]._id%>">
        <button type="button" class="btn btn-warning">Edit  <i class="fa-solid fa-pen"> </i></button></a>


        
 <a class="btn btn-danger delete" onclick ="deletefunction('<%= users[i]._id %>')" data-id=<%= users[i]._id%>> Delete
    <span class="text-gradient"><i class="fas fa-times"></i></span>
</a>
   </td>
</tr>
<% } %>

  

